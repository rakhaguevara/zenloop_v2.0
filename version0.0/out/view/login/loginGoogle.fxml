<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="654.0" prefWidth="999.0" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GoogleLoginController">
    <children>
        <ImageView fitHeight="654.0" fitWidth="999.0">
            <image>
                <Image url="@../../app/resource/loginSoon.png" />
            </image>
        </ImageView>

        <Button fx:id="btnBackLoginToPage" layoutX="419.0" layoutY="321.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="55.0" prefWidth="183.0" style="-fx-background-color: #1A202C;" text="Back To Login" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>

        <Button fx:id="btnRegisterPage" layoutX="511.0" layoutY="259.0" mnemonicParsing="false" onAction="#handleRegister" prefHeight="32.0" prefWidth="95.0" style="-fx-background-color: transparent; -fx-border-color: transparent;" text="register" textFill="#170000">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
