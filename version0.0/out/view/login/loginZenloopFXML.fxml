<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="654.0" prefWidth="999.0" style="-fx-background-color: #fff;"
   xmlns="http://javafx.com/javafx/8"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginControllerZenloop">
   <children>
      <ImageView fx:id="leftImage" fitHeight="634.0" fitWidth="482.0" layoutX="11.0" layoutY="11.0">
         <image>
            <Image url="@../../app/resource/left.png" />
         </image>
      </ImageView>
      <ImageView fx:id="rightImage" fitHeight="635.0" fitWidth="487.0" layoutX="503.0" layoutY="10.0">
         <image>
            <Image url="@../../app/resource/right.png" />
         </image>
      </ImageView>

      <TextField fx:id="tfUsername" layoutX="74.0" layoutY="205.0" prefHeight="49.0" prefWidth="357.0" promptText="Username" />

      <PasswordField fx:id="tfPassword" layoutX="73.0" layoutY="291.0" prefHeight="45.0" prefWidth="359.0" promptText="Password" />

      <Button fx:id="btnLoginToPage" layoutX="74.0" layoutY="367.0" onAction="#handleLogin" mnemonicParsing="false" prefHeight="44.0" prefWidth="357.0" style="-fx-background-color: #1A202C;" text="Login" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>

      <Button fx:id="btnLoginWGoogle" layoutX="75.0" layoutY="459.0" mnemonicParsing="false" onAction="#handleGoogleLogin" prefHeight="42.0" prefWidth="356.0" style="-fx-background-color: transparent;" textFill="BLACK">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>

      <Button fx:id="btnRegisterPage" layoutX="279.0" layoutY="515.0" mnemonicParsing="false" onAction="#handleRegister" prefHeight="27.0" prefWidth="71.0" style="-fx-background-color: transparent; -fx-border-color: transparent;" text="Sign Up" textFill="#170000">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
